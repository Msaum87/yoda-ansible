[composite:main]
use = egg:Paste#urlmap
/oai = yoda_moai

[app:yoda_moai]
use = egg:moai
name = MOAI YoDa Feed
url = https://{{ yoda_public_fqdn }}/oai
admin_email = fb-yoda@uu.nl
formats = oai_dc mods
disallow_sets = private
database = sqlite:///{{ yoda_moai_home }}/moai.db
provider = file://{{ yoda_metadata_inbox }}
content = moai_yoda
forcedflush = 10000

[server:main]
use = egg:PasteScript#wsgiutils
host = 127.0.0.1
port = 8080

